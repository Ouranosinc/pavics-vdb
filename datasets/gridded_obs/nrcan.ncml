		    <aggregation dimName="time" type="joinExisting" recheckEvery="1 day">
			    <scan location="/birdhouse/nrcan/nrcan_canada_daily/tasmin" dateFormatMark="nrcan_canada_daily_tasmin_#yyyy" suffix=".nc" subdirs="false"/>
		    </aggregation>
	    </netcdf>
	    <netcdf>
		    <aggregation dimName="time" type="joinExisting" recheckEvery="1 day">
			    <scan location="/birdhouse/nrcan/nrcan_canada_daily/tasmax" dateFormatMark="nrcan_canada_daily_tasmax_#yyyy" suffix=".nc" subdirs="false"/>
		    </aggregation>
	    </netcdf>
	    <netcdf>
		    <aggregation dimName="time" type="joinExisting" recheckEvery="1 day">
			    <scan location="/birdhouse/nrcan/nrcan_canada_daily/pr" dateFormatMark="nrcan_canada_daily_pr_#yyyy" suffix=".nc" subdirs="false"/>
		    </aggregation>
	    </netcdf>
    	</aggregation>
</netcdf>
