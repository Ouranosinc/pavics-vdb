<?xml version="1.0" encoding="utf-8"?>
<netcdf xmlns="http://www.unidata.ucar.edu/namespaces/netcdf/ncml-2.2">
	<remove name="model_id" type="attribute"></remove>
	<remove name="experiment" type="attribute"></remove>
	<remove name="downscalingModel" type="attribute"></remove>
